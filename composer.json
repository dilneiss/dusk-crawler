{
    "name": "dilneiss/dusk-crawler",
    "description": "Web Crawler using Laravel Dusk",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "KATSANA Developer",
            "email": "developer@katsana.com"
        },
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "DuskCrawler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DuskCrawler\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=8.0",
        "laravel/dusk": "^5.11 || ^6.0",
        "orchestra/canvas-core": "^4.7 || ^5.0 || ^6.0 || ^7.0 || ^8.0",
        "orchestra/dusk-updater": "^1.2",
        "react/promise": "^2.7",
        "symfony/dom-crawler": "^4.3 || ^5.0"
    },
    "require-dev": {
        "orchestra/canvas": "^4.5 || ^5.0 || ^6.0 || ^7.0",
        "orchestra/testbench": "^4.5 || ^5.0 || ^6.0 || ^7.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DuskCrawler\\DuskServiceProvider"
            ]
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
